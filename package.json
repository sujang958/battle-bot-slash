{
  "name": "battlebot",
  "main": "src/index.ts",
  "repository": "https://github.com/kiss8981/battle-bot-slash",
  "author": "Kisss",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --transpile-only --respawn -r tsconfig-paths/register --watch ./src/**/*.pug,./src/**/*.graphql --trace-warnings --trace-uncaught -- ./src/index.ts",
    "start": "NODE_ENV=production ts-node --transpile-only -r tsconfig-paths/register src/index.ts",
    "watch": "tsc --watch",
    "build": "tsc",
    "test": "eslint ./ --fix",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix"
  },
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "chalk": "2.4.1",
    "dateformat": "^5.0.2",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.4.0",
    "dokdo": "^0.5.1",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "module-alias": "^2.2.2",
    "mongodb": "^4.3.1",
    "mongoose": "^6.0.13",
    "node-fetch": "^3.2.0",
    "quick.db": "^7.1.3",
    "randomstring": "^1.2.2",
    "strip-ansi": "^6.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.10",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.1.0",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "tspath": "^1.3.7",
    "typescript": "^4.5.5"
  },
  "_moduleAliases": {
    "@utils/": "./src/utils/",
    "@config": "./config.ts",
    "@client": "./src/structures/BotClient.ts",
    "@events/": "./src/events/",
    "@commands/": "./src/commands/",
    "@types": "./types/index.d.ts",
    "@managers/": "./src/managers/",
    "@schemas/": "./src/schemas/"
  },
  "eslintIgnore": [
    "node_modules",
    "src/commands/",
    "example.*"
  ]
}
